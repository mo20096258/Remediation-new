{
  "name": "asp.net",
  "private": true,
  "dependencies": {
  },
  "AppSettings": {
    "DisposeRule": "true",
    // Provide a private default Constructor for utility Classes (.NET).
    "PrivateConstructor": "true",
    //Avoid using a break statement in 'for' loops.
    "BreakInFor": "true",
    //Avoid having transaction with the Thread.Sleep method in.
    "ThreadSleep": "true",
    //Avoid empty finally blocks.
    "EmptyFinal": "true",
    //Avoid empty catch blocks.
    "EmptyCatch": "true",
    //Avoid String concatenation in loops (.NET)
    "StringConcatenationInLoop": "true",
    //Avoid large number of String concatenation (.NET)
    "LargeStringConcatenation": "true",
    //Avoid using String.Empty for empty string tests.
    "StringEmpty": "true",
    //Avoid instantiations inside loops (.NET).
    "InstantiationsFor": "true",
    //Avoid uncontrolled format string
    "Formatter": "true",
    //Avoid declaring public Fields.
    "PublicField": "true",
    //Remove Artifacts that are not use.
    "UnreferencedArtifacts": "true",
    //Avoid large Classes - too many Methods (.NET)
    "LargeClass": "true",
    //Avoid unreferenced Forms
    "Unreferenced": "true",
    //Avoid large Classes - too many Constructors (.NET) .
    "ManyConstructors": "true",
    //SQL connection should be closed within the Method / Function / Sub that opened it .
    "SQLConnection": "true",
    //Avoid large Methods - too many Lines of Code
    "LargeMethod": "true",
    "AvoidSingleCharVariable": "true",
    "AvoidUnderscore": "false",
    "CamelCaseNaming": "false",
    "DirectAccesstoDbCheck": "true",
    "DisableConstraintsBeforeMergingDataSet": "true",
    "AvoidcalltoAcceptChangesinaloop": "true",
    "AvoidUsingGotoStatement": "true",
    "AvoidArtifactsWithTooManyParameters": "true",
    "Namespaceuppercase": "true",
    "PropertiesUppercase": "true",
    "nuget": "true",
    "UnusedVariables": "true",
    "CollapsibleIfToBeMerged": "true",
    "SwitchToHaveAtleastThreeCases": "true",
    "UtilityClassesNotInstantiated": "true",
    "DuplicateImplementation": "true",
    //"IndexOf" checks should not be for positive numbers
    "IndexOfChecks": "true",
    //A "for" loop update clause should move the counter in the right direction
    "ForLoopCounterRightDirection": "true",
    //Non-constant static fields should not be visible
    "NonConstantStaticField": "true",
    //"GetHashCode" should not reference mutable fields
    "GetHashCodeMutableField": "true",
    //Empty arrays and collections should be returned instead of null
    "EmptyArrayandCollection": "true",
    //"string.IsNullOrEmpty" should be used
    "stringIsNullOrEmpty": "true",
    //"==" should not be used when "Equals" is overridden
    "EqualsOverridden": "true",
    //Right operands of shift operators should be integers
    "RightOperandOfShiftOperator": "true",
    //Methods should not have too many parameters
    "AvoidMethodwithManyParameters": "true"



  }
}
